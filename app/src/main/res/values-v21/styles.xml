<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Base application theme. -->
    <style name="Botty.Install.Theme" parent="Theme.AppCompat">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:colorPrimaryDark">@color/material_blue_grey_800</item>
        <item name="android:colorPrimary">@color/material_blue_gray_500</item>
        <item name="android:colorAccent">@color/material_blue_gray_200</item>
        <item name="android:colorActivatedHighlight">@color/material_blue_gray_200</item>
        <item name="android:colorEdgeEffect">@color/material_blue_gray_200</item>
    </style>

    <style name="Botty.Install.Theme.Light" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:colorPrimaryDark">@color/material_blue_grey_800</item>
        <item name="android:colorPrimary">@color/material_blue_gray_500</item>
        <item name="android:colorAccent">@color/material_blue_gray_200</item>
        <item name="android:colorActivatedHighlight">@color/material_blue_gray_200</item>
        <item name="android:colorEdgeEffect">@color/material_blue_gray_200</item>
    </style>
</resources>